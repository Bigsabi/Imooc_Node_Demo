extends ../layout

block content
  .container
    .row
      .col-md-7
        embed(src="#{movie.flash}",allowFullScreen="true",quality="high",width="720",height="600",aligh="middle",type="application/x-shockwave-flash")
        .panel.panel-defatul
          .panel-heading
            h3 评论区
          .panel-body
            ul.media-list
              each item in comments
                li.meida
                  .pull-left
                    a.comment(href="#comments",data-cid="#{item._id}",data-tid="#{item.from._id}")
                      img.media-object(src="data:image/png;base64,",style="width:64px;height:64px;")
                .media-body
                  h4.media-heading #{item.from.name}
                  p #{item.content}
                  if item.reply && item.reply.length > 0
                    each reply in item.reply
                      .media
                        .pull-left
                          a.comment(href="#comments",data-cid="#{item._id}",data-tid="#{item.from._id}")
                            img.media-object(src="",style="width:64px;height:64px;")
                        .media-body
                          h4.media-heading 
                            .span.tet-info #{reply.from.name} &nbsp;回复&nbsp;#{reply.to.name} :
                          p #{reply.content}
                hr
        #comments
          form#commentForm(method="POST",action="/user/comment")
            input(type="hidden",name="comment[movie]" value="#{movie._id}")
            .form-group
              textarea.form-control(name="comment[content]",row="3")
            if user
              input(type="hidden",name="comment[from]" value="#{user._id}")
              input(type="hidden",name="comment[to]" value="#{user._id}")
              button.btn.btn-primary(type="submit") 提交
            else
              a.btn.btn-primary(href="/signin") 登陆以评论
      .col-md-5
        dl.dl-horizontal
          dt 电影名字
          dd=movie.title
          dt 导演
          dd=movie.doctor
          dt 国家
          dd=movie.country
          dt 语言
          dd=movie.language
          dt 上映年份
          dd=movie.year
          dt 简介
          dd=movie.summary
  script(src="/js/detail.js")